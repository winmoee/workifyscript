{
    "manifest_version": 2,
    "name": "Tab Manager for Chrome Dev Docs",
    "version": "1.0",
    "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "permissions": [
        "tabs",
        "*://*.jackandamydev.com/*",
        "*://localhost/*",
        "*://127.0.0.1/*"
    ],
    "host_permissions": [
        "*://*.jackandamydev.com/*",
        "*://localhost/*",
        "*://127.0.0.1/*"
    ]
}